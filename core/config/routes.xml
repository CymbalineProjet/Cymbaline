<?xml version='1.0' encoding='UTF-8'?>

<routes>  
    <import>
        <file>
            <source>/source/Alca/GenBox/config/generator.xml</source>
        </file>
    </import>
      
        
    <route name="index" path="/" 
           template="Alca/GenBox/template/index" controller="Alca/GenBox/Defaut" action="index" />

    <route name="error" path="/error" 
           template="Cymbaline/Error/template/error" controller="Cymbaline/Error/Error" action="index" />
    
    <route name="generator_todo" path="/generator/todo" 
           template="Alca/GenBox/template/todo/index" controller="Alca/GenBox/Todo" action="todo" />        
        
</routes>
